@model innovation4austria.web.Models.ProfileModel
@{
    ViewBag.Title = "innovation4austria - Profil verwalten";
}

<div class="row">
    <div class="col-xs-12">
        <h1>Profil verwalten</h1>
    </div>
</div>
<div class="row">
    <div class="col-md-offset-1 col-md-5 col-xs-12">
        @Html.Partial("_ProfileData", Model.ProfileData)
    </div>
    <div class="col-md-offset-1 col-md-5 col-xs-12">
        @Html.Partial("_ProfilePassword", Model.ProfilePassword)
    </div>
</div>


@section scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
